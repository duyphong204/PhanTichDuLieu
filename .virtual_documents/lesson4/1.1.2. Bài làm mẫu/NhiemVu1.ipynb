import numpy as np
import pandas as pd
from scipy import stats
import matplotlib.pyplot as plt
import seaborn as sns



# Hi·ªÉn th·ªã ƒë·∫ßy ƒë·ªß c·ªôt
pd.set_option('display.max_columns', None)

# üîπ 2. ƒê·ªåC D·ªÆ LI·ªÜU
covid_data = pd.read_csv("data/compact.csv")

# Ch·ªçn m·ªôt s·ªë c·ªôt c·∫ßn thi·∫øt cho b√†i ph√¢n t√≠ch
covid_data = covid_data[['country', 'continent', 'date', 'total_cases', 'new_cases', 'total_deaths', 'new_deaths']]

# Xem 5 d√≤ng ƒë·∫ßu
print("üìå 5 d√≤ng ƒë·∫ßu ti√™n:")
print(covid_data.head())

print("\nüìä Ki·ªÉu d·ªØ li·ªáu v√† th√¥ng tin:")
print(covid_data.info())

print("\nüìè K√≠ch th∆∞·ªõc t·∫≠p d·ªØ li·ªáu:", covid_data.shape)





# Lo·∫°i b·ªè gi√° tr·ªã NaN ·ªü c·ªôt 'new_cases' ƒë·ªÉ t√≠nh to√°n ch√≠nh x√°c
new_cases = covid_data['new_cases'].dropna()

# T√≠nh c√°c ch·ªâ s·ªë th·ªëng k√™
data_mean = np.mean(new_cases)
data_median = np.median(new_cases)
data_mode = stats.mode(new_cases, keepdims=True)[0][0]
data_variance = np.var(new_cases)
data_sd = np.std(new_cases)
data_max = np.max(new_cases)
data_min = np.min(new_cases)
data_range = data_max - data_min
data_percentile = np.percentile(new_cases, 60)
data_quartiles = np.percentile(new_cases, [25, 50, 75])
data_IQR = stats.iqr(new_cases)

# In k·∫øt qu·∫£
print("\nüîπ TH·ªêNG K√ä M√î T·∫¢ C·ªòT 'new_cases'")
print(f"Mean (Trung b√¨nh): {data_mean:.2f}")
print(f"Median (Trung v·ªã): {data_median:.2f}")
print(f"Mode (Mode): {data_mode}")
print(f"Variance (Ph∆∞∆°ng sai): {data_variance:.2f}")
print(f"Standard Deviation (ƒê·ªô l·ªách chu·∫©n): {data_sd:.2f}")
print(f"Range (Kho·∫£ng gi√° tr·ªã): {data_range:.2f}")
print(f"60th Percentile: {data_percentile:.2f}")
print(f"Quartiles (Q1, Q2, Q3): {data_quartiles}")
print(f"IQR (Kho·∫£ng t·ª© ph√¢n v·ªã): {data_IQR:.2f}")






# Ki·ªÉm tra gi√° tr·ªã thi·∫øu
print("\nüîç S·ªë l∆∞·ª£ng gi√° tr·ªã thi·∫øu theo t·ª´ng c·ªôt:")
print(covid_data.isnull().sum())

# Histogram ‚Äì ph√¢n b·ªë s·ªë ca m·∫Øc m·ªõi
plt.figure(figsize=(8,5))
plt.hist(new_cases, bins=50, color='skyblue', edgecolor='black')
plt.title("Ph√¢n b·ªë s·ªë ca m·∫Øc m·ªõi (new_cases)")
plt.xlabel("S·ªë ca m·∫Øc m·ªõi")
plt.ylabel("T·∫ßn su·∫•t xu·∫•t hi·ªán")
plt.show()

# Boxplot ‚Äì ph√°t hi·ªán gi√° tr·ªã ngo·∫°i lai
plt.figure(figsize=(6,4))
sns.boxplot(x=new_cases, color='lightcoral')
plt.title("Boxplot ‚Äì Ph√°t hi·ªán ngo·∫°i lai (new_cases)")
plt.show()









continent_cases = covid_data.groupby('continent')['total_cases'].max().dropna().sort_values(ascending=False)
plt.figure(figsize=(8,5))
continent_cases.plot(kind='bar', color='teal')
plt.title("T·ªïng s·ªë ca m·∫Øc theo ch√¢u l·ª•c")
plt.ylabel("T·ªïng ca m·∫Øc")
plt.show()






plt.figure(figsize=(7,5))
sns.scatterplot(data=covid_data, x='total_cases', y='total_deaths', alpha=0.5)
plt.title("M·ªëi quan h·ªá gi·ªØa t·ªïng ca m·∫Øc v√† t·ªïng ca t·ª≠ vong")
plt.xlabel("T·ªïng ca m·∫Øc (total_cases)")
plt.ylabel("T·ªïng ca t·ª≠ vong (total_deaths)")
plt.show()





corr = covid_data[['total_cases', 'total_deaths', 'new_cases', 'new_deaths']].corr()
print("\nüîπ Ma tr·∫≠n t∆∞∆°ng quan:")
