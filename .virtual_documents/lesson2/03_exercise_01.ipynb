def DemTongChanLe(a):
    """
    Input: a[] = [1, 4, 2, 0, -1, -7, 3, -6, ...]
    Output: tong, sochan, sole
    """
    tong = sum(a)
    sochan = sum(1 for x in a if x % 2 == 0)
    sole = len(a) - sochan
    return tong, sochan, sole
# DemTongChanLe


