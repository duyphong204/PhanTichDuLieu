import random
def SinhNgauNhien(n, vmin=-10, vmax=10):
    """
    Input:
        n = 30, vmin = -10, vmax = 10
    Output:
        a = [ngau nhien...]
    """
    a = [random.randint(vmin, vmax) for _ in range(n)]
    return a
# SinhNgauNhien


